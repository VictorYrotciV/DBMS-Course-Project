<!--pages/addReader/addReader.wxml-->
<!--pages/about/about.wxml-->
<view>
  <view id="look">
    <text class="tishi">输入证件号码进行搜索</text>
  </view>
  <form bindsubmit="searchid">
    <view class="msginput">
      <input id="input" name="id" type="text"  placeholder="请输入证件号码"/>
    </view>
    <button form-type="submit" >搜索</button>
  </form>
  <view class="blank"></view>
  <form bindsubmit="submit">
    <text class="tishi">输入想要删除的读者的证件号码</text>
    <view class="msginput">
      <input id="input" name="id" type="text"  placeholder="请输入证件号码"/>
    </view>
    <button form-type="submit" >删除</button>
  </form>
  <view id="result" class="rst1">
    <text class="tishi">结果：</text>
    <textarea name=""  id="out" cols="30" rows="10" class="msginput">{{text}}</textarea>
  </view>
  <button id="bottom" bindtap="back">返回</button>
</view>

